[
  {
    "$id": "base-container",
    "Default": {
      "BackgroundColor": [ "#121212ff" ],
      "BackgroundShader": "shader-ui",
      "CornerRadius": [ [ 8, 8 ] ]
    }
  },
  {
    "$id": "base-block",
    "Default": {
      "BackgroundColor": [ "#ffffff1c" ],
      "BackgroundShader": "shader-ui",
      "CornerRadius": [ [ 8, 8 ] ]
    }
  },
  {
    "$id": "base-span",
    "Parent": "base-container",
    "Default": {
      "FontFace": "font-syne-mono",
      "Color": "#ffffffdf",
      "Shader": "shader-default",
      "DisableScissor": true
    }
  },
  {
    "$id": "base-tex-button",
    "Default": {
      "BackgroundColor": [ "#ffcc80ff" ],
      "ForegroundColor": "#121212ff",
      "Color": "#121212ff",
      "BorderWidth": [ 1 ],
      "BorderColor": [ "#ffffff00" ],
      "BackgroundShader": "shader-ui",
      "DisableScissor": true
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#fea339ff" ]
        }
      },
      {
        "State": [ "Toggle" ],
        "Attributes": {
          "BorderColor": [ "#ffffffff" ]
        }
      }
    ]
  }
]