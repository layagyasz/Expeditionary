[
  {
    "$id": "base-container",
    "Default": {
      "BackgroundColor": [ "#26333bdf" ],
      "BackgroundShader": "shader-ui",
      "BorderWidth": [ 0, 2, 0, 2 ],
      "BorderColor": [ "#5e7d78c1" ]
    }
  },
  {
    "$id": "base-block",
    "Default": {
      "BackgroundColor": [ "#26333b00" ],
      "BackgroundShader": "shader-ui"
    }
  },
  {
    "$id": "base-span",
    "Default": {
      "BackgroundColor": [ "#26333b00" ],
      "BackgroundShader": "shader-ui",
      "FontFace": "font-syne-mono",
      "Color": "#c08b57c1",
      "Shader": "shader-default",
      "DisableScissor": true
    }
  },
  {
    "$id": "base-button",
    "Parent": "base-span",
    "Default": {
      "Size": [ 388, 36 ],
      "Margin": [ 2 ],
      "Padding": [ 2 ],
      "BackgroundColor": [ "#26333b00" ],
      "BorderWidth": [ 2 ],
      "BorderColor": [ "#c08b57c1" ],
      "BackgroundShader": "shader-ui",
      "DisableScissor": true,
      "FontSize": 12,
      "Color": "#c08b57c1",
      "Align": "Center",
      "SfxMouseOver": "sfx-button-mouseover",
      "SfxClick": "sfx-button-click"
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BorderColor": [ "#c08b57ff" ],
          "Color": "#c08b57ff"
        }
      },
      {
        "State": [ "Toggle" ],
        "Attributes": {
          "BackgroundColor": [ "#5e7d78c1" ]
        }
      },
      {
        "State": [ "Disable" ],
        "Attributes": {
          "BorderColor": [ "#c08b5788" ],
          "Color": "#c08b5788"
        }
      }
    ]
  },
  {
    "$id": "base-button-image",
    "Default": {
      "Size": [ 32, 32 ],
      "BackgroundColor": [ "#ffffff00" ],
      "BackgroundShader": "shader-ui",
      "ForegroundColor": "#c08b57c1"
    }
  },
  {
    "$id": "base-button-text",
    "Parent": "base-span",
    "Default": {
      "Size": [ 332, 32 ],
      "FontSize": 16,
      "Padding": [ 8 ],
      "Color": "#c08b57c1",
      "Align": "Left"
    }
  }
]