[
  {
    "$id": "base-container",
    "Default": {
      "BackgroundColor": [ "#26333bee" ],
      "BackgroundShader": "shader-ui",
      "BorderWidth": [ 2 ],
      "BorderColor": [ "#5e7d78c1" ]
    }
  },
  {
    "$id": "base-element",
    "Default": {
      "BackgroundShader": "shader-ui",
      "FontFace": "font-syne-mono",
      "Color": "#5e7d78c1",
      "Shader": "shader-default",
      "DisableScissor": true
    }
  },
  {
    "$id": "base-block",
    "Parent": "base-element",
    "Default": {
      "BackgroundColor": [ "#384b49df" ],
      "BorderWidth": [ 0, 0, 0, 2 ],
      "BorderColor": [ "#5e7d78c1" ],  
      "Align": "Center"
    }
  },
  {
    "$id": "base-span",
    "Parent": "base-element",
    "Default": {
      "BackgroundColor": [ "#26333b00" ],
      "Align": "Center"
    }
  },
  {
    "$id": "base-button",
    "Parent": "base-span",
    "Default": {
      "BackgroundColor": [ "#26333b00" ],
      "BorderWidth": [ 2 ],
      "BorderColor": [ "#5e7d78c1" ],
      "Color": "#5e7d78c1",
      "Align": "Center",
      "SfxMouseOver": "sfx-button-mouseover",
      "SfxClick": "sfx-button-click"
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BorderColor": [ "#5e7d78ff" ],
          "Color": "#5e7d78ff"
        }
      },
      {
        "State": [ "Toggle" ],
        "Attributes": {
          "BackgroundColor": [ "#384b49c1" ]
        }
      },
      {
        "State": [ "Disable" ],
        "Attributes": {
          "BorderColor": [ "#5e7d7888" ],
          "Color": "#5e7d7888"
        }
      }
    ]
  },
  {
    "$id": "base-button-image",
    "Default": {
      "Size": [ 32, 32 ],
      "BackgroundColor": [ "#ffffff00" ],
      "BackgroundShader": "shader-ui",
      "ForegroundColor": "#5e7d78c1"
    }
  },
  {
    "$id": "base-button-text",
    "Parent": "base-span",
    "Default": {
      "Size": [ 332, 32 ],
      "FontSize": 16,
      "Padding": [ 8 ],
      "Color": "#5e7d78c1",
      "Align": "Left"
    }
  }
]